{
  "name": "simple-staking",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "clean": "rm -r node_modules",
    "build-docker": "docker build -t babylonlabs/simple-staking -f docker/Dockerfile .",
    "clean-docker": "docker rmi babylonlabs/simple-staking 2>/dev/null; true",
    "prepare": "husky",
    "sort-imports": "eslint --fix .",
    "test": "node --trace-deprecation ./node_modules/.bin/jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report"
  },
  "engines": {
    "node": "22.3.0"
  },
  "dependencies": {
    "@babylonlabs-io/babylon-proto-ts": "1.0.1",
    "@babylonlabs-io/core-ui": "1.0.0",
    "@babylonlabs-io/wallet-connector": "1.0.12",
    "@babylonlabs-io/btc-staking-ts": "1.0.3",
    "@bitcoin-js/tiny-secp256k1-asmjs": "2.2.3",
    "@bitcoinerlab/secp256k1": "^1.1.1",
    "@cosmjs/proto-signing": "^0.32.4",
    "@cosmjs/stargate": "^0.32.4",
    "@sentry/nextjs": "^8.30.0",
    "@tanstack/react-query": "^5.28.14",
    "@tanstack/react-query-next-experimental": "^5.28.14",
    "@uidotdev/usehooks": "^2.4.1",
    "bitcoinjs-lib": "6.1.5",
    "core-js": "^3.41.0",
    "date-fns": "^3.6.0",
    "decimal.js-light": "^2.5.1",
    "framer-motion": "^11.1.9",
    "next": "14.2.25",
    "next-themes": "^0.3.0",
    "qs": "^6.13.1",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.1.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-number-format": "^5.4.2",
    "react-responsive-modal": "^6.4.2",
    "react-tabs": "^6.0.2",
    "react-toastify": "^10.0.6",
    "react-tooltip": "^5.26.4",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.5.2",
    "url-safe-base64": "^1.3.0",
    "usehooks-ts": "^3.0.2",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@playwright/test": "^1.46.0",
    "@tanstack/eslint-plugin-query": "^5.28.11",
    "@tanstack/react-query-devtools": "^5.28.14",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/qs": "^6.9.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "daisyui": "^4.11.1",
    "ecpair": "^2.1.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.0",
    "msw": "^1.2.1",
    "postcss": "^8",
    "postcss-import": "^16.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "3.4.3",
    "text-encoding": "^0.7.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "overrides": {
    "whatwg-url@11.0.0": {
      "tr46": "4.0.0"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{js,jsx,ts,tsx,json,css,scss,md,yml,yaml}": [
      "prettier --write --ignore-unknown"
    ]
  }
}
